<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shiptracker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <permission
        android:name="com.shiptracker.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.shiptracker.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <!--
     The following two permissions are not required to use
     Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.shiptracker.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shiptracker.Start"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shiptracker.Ports"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.PORTS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.shiptracker.Instructions"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.INSTRUCTIONS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.shiptracker.Credits"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.CREDITS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shiptracker.Journey"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.JOURNEY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.shiptracker.NearestPorts"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.NEARESTPORTS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.shiptracker.Speed"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.SPEED" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shiptracker.TrackJourney"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.TRACKJOURNEY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shiptracker.Options"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.shiptracker.OPTIONS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
        


        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyD7FBU-oItBTZQ9BVcWCCNIyLke72EI8h8" />
    </application>

</manifest>